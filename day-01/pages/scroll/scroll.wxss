.contain2 {
  border: solid 1px red;
  width: 100px;
  height: 120px;
}
.contain2 view{
  width: 100px;
  height: 100px;
  text-align: center;
  line-height: 100px;
}
.contain2 view:nth-child(1) {
  background-color: pink;
}
.contain2 view:nth-child(2) {
  background-color: rebeccapurple;
}
.contain2 view:nth-child(3) {
  background-color: sandybrown;
}
